<?xml version="1.0" ?>
<robot name="robot_name">
    <webots>
        <device reference="LDS-01" type="Lidar">
            <tiltAngle>0.0</tiltAngle>
            <horizontalResolution>512</horizontalResolution>
            <fieldOfView>1.5708</fieldOfView>
            <verticalFieldOfView>0.2</verticalFieldOfView>

            <!-- Using the official Webots ROS 2 interface -->
            <ros>
                <enabled>true</enabled>
                <updateRate>5</updateRate>
                <topicName>/scan</topicName>
                <alwaysOn>false</alwaysOn>
                <frameName>ds0</frameName>
            </ros>
        </device>

        <device reference="camera" type="Camera">
            <ros>
                <updateRate>33</updateRate>
                <alwaysOn>true</alwaysOn>
                <frameName>ds0</frameName>
            </ros>
        </device>

        <device reference="camera_1" type="Camera">
